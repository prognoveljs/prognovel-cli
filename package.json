{
  "name": "@prognovel/cli",
  "version": "0.8.9",
  "description": "A repo to manage contents of ProgNovel platform.",
  "main": ".prognovel/bin",
  "bin": {
    "prognovel": ".prognovel/bin/index.js"
  },
  "preferGlobal": true,
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:both": "npm run build:types && npm run build",
    "build": "rollup -c",
    "dev": "node .prognovel/.development/server.js",
    "format": "prettier --write .prognovel/**/*.{ts,js,md}",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prognoveljs/prognovel-cli.git"
  },
  "author": "ProgNovel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prognoveljs/prognovel-cli/issues"
  },
  "homepage": "https://github.com/prognoveljs/prognovel-cli#readme",
  "dependencies": {
    "alphanum-sort": "^1.0.2",
    "archiver": "^5.3.0",
    "brotli": "^1.3.2",
    "bufferutil": "^4.0.6",
    "chalk": "^4.1.2",
    "dessert-yaml-js": "^1.0.3",
    "dotenv": "^10.0.0",
    "front-matter": "^4.0.2",
    "glob": "^7.2.0",
    "glob-promise": "^4.2.2",
    "globby": "^12.0.2",
    "image-type": "^4.1.0",
    "is-image": "^3.0.0",
    "js-yaml": "^4.1.0",
    "markdown-wasm": "^1.2.0",
    "node-fetch": "^2.6.6",
    "node-html-markdown": "^1.1.3",
    "polka": "^0.5.2",
    "promisify-child-process": "^4.1.1",
    "readline-promise": "^1.0.5",
    "sharp": "^0.28.3",
    "source-map-support": "^0.5.21",
    "string-similarity": "^4.0.4",
    "tiny-glob": "^0.2.9",
    "toml": "^3.0.0",
    "utf-8-validate": "^5.0.8",
    "wasm-frontmatter": "^1.0.1",
    "ws": "^8.4.0",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-transform-regenerator": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@rollup/plugin-wasm": "^5.1.2",
    "@types/js-yaml": "^4.0.5",
    "@types/ws": "^8.2.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.25.3",
    "prettier": "^2.5.1",
    "rollup": "^2.60.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-regenerator": "^0.6.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.5.2"
  }
}
